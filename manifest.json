{
  "manifest_version": 2,
  "name": "New Tab UI",
  "description": "New Tab Override",
  "author": "Alexandru Jeman",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "offline_enabled": true,
  "incognito": "spanning",
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["./js/newtab.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "./options.html",
    "open_in_tab": false
  }
}